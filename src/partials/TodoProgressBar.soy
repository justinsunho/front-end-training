{namespace TodoProgressBar}

/**
 */
{template .render}
	{@param? percentage: ?}

	<h3>
		Completion
	</h3>

	<div class="progress">
		<div aria-valuemax="100"  aria-valuemin="0" aria-valuenow="{$percentage}" class="progress-bar" role="progressbar" style="width: {$percentage}%;"></div>
	</div>
{/template}